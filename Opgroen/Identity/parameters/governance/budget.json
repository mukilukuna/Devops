{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "name": {
      "value": "*<subscriptionName>*-Budget"
    },
    "amount": {
      "value": *<budget>*
    },
    "timeGrain": {
      "value": "Monthly"
    },
    "operator": {
      "value": "GreaterThan"
    },
    "threshold": {
      "value": 90
    },
    "forecastThreshold": {
      "value": 120
    },
    "startDate": {
      "value": "*<budgetStartDate>*"
    },
    "contactEmails": {
      "value": [
        "*<budgetEmail>*"
      ]
    },
    "contactRoles": {
      "value": []
    },
    "contactGroups": {
      "value": []
    }
  }
}